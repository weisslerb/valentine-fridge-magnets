package com.brandseduction {  	import flash.display.MovieClip;	import flash.display.Stage;	import caurina.transitions.Tweener;  import com.brandseduction.Tile;	public class TilingBackground extends MovieClip {    var _tile_width = 200;    var _tile_height = 165;	  		function TilingBackground() {		  alpha = 0;    }        public function init() {      trace ("tiling_background init")            var x_max = Math.round(8000/_tile_width);      var y_max = Math.round(5000/_tile_height);      for (var ix=0; ix<x_max; ix++) {        for (var iy=0; iy<y_max; iy++) {          var tile:Tiler = new Tiler(_tile_width*ix, _tile_height*iy);          this.addChild(tile);        }      }            Tweener.addTween(this, {alpha:1, time:1, delay:0, transition:"easeInCubic"});    }    	}	}