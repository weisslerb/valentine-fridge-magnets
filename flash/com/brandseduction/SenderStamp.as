package com.brandseduction {  	import flash.display.*;	import caurina.transitions.Tweener;	import de.popforge.events.SimpleMouseEvent;	import de.popforge.events.SimpleMouseEventHandler;	public class SenderStamp extends MovieClip {		function SenderStamp() {			buttonMode = true;       mouseChildren = false;			SimpleMouseEventHandler.register(this);			addEventListener( SimpleMouseEvent.RELEASE, releaseHandler);		}				public function init() {	    graphics.beginFill(0x00FF00);	    graphics.drawRect(0, 0, 150, 100);	    graphics.endFill();				x = parent.width/2 - width - 30			y = -parent.height/2 + 30			alpha = 0		}				private function releaseHandler(event:SimpleMouseEvent):void {			parent.parent.getChildByName("fridge").save_magnet_data();		}			}}