package com.brandseduction {    import flash.display.*;	import flash.filters.*;	import caurina.transitions.Tweener;		public class Purchase extends MovieClip {				var _door;		var _hanger;		var _knob;		var _picture;	  		function Purchase() {		}				public function init() {      			y = 0			x = parent._scene_positions["purchase"];			      var door:PurchaseDoor = new PurchaseDoor();      this.addChild(door)      door.init();			_door = door;			      var hanger:PurchaseDoorHanger = new PurchaseDoorHanger();      this.addChild(hanger)      hanger.init();			_hanger = hanger;			      var knob:PurchaseDoorKnob = new PurchaseDoorKnob();      this.addChild(knob)      knob.init();			_knob = knob;			      var picture:PurchasePicture = new PurchasePicture();      this.addChild(picture)      picture.init();			_picture = picture;		}				public function shake() {			_hanger.shake();			_picture.shake();		}				public function blow_away() {			_hanger.blow_away();			_picture.blow_away();					}			}	}