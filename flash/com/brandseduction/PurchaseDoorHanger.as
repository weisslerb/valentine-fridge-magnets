package com.brandseduction {  	import flash.display.*;	import flash.filters.*;	import caurina.transitions.Tweener;	import flash.events.*;	public class PurchaseDoorHanger extends MovieClip {	  		function PurchaseDoorHanger() {    }        public function init() {			x = parent._door.x + 350;			y = parent._door.y + 372;				adapt_to_stage_scaling();			drop_shadow();    }				function drop_shadow() {      var color:Number = 0x000000;      var angle:Number = 45;      var alpha:Number = 1;      var blurX:Number = 4;      var blurY:Number = 4;      var distance:Number = 2;      var strength:Number = 0.85;      var inner:Boolean = false;      var knockout:Boolean = false;      var quality:Number = BitmapFilterQuality.HIGH;      var s:DropShadowFilter = new DropShadowFilter(distance, angle, color, alpha, blurX, blurY, strength, quality, inner, knockout);      filters = [s];		}				public function adapt_to_stage_scaling() {		}				public function shake() {			rotation = 120;			Tweener.addTween(this, {rotation:0, time:8, transition:"easeOutElastic"})		}				public function blow_away() {			Tweener.addTween(this, {rotation:-45, time:1, transition:"easeInCubic"})			Tweener.addTween(this, {rotation:0, time:.5, delay:1, transition:"easeOutCubic"})		}			}	}