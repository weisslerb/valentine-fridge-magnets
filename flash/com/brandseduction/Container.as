package com.brandseduction {    import flash.display.*;  import flash.events.*;	import caurina.transitions.Tweener;		public class Container extends MovieClip {	  		var _fridge;		var _logo;		var _footer;		var _scene;			function Container() {    }                             public function init() {      trace ("container init");       // Configure stage      stage.align = StageAlign.TOP_LEFT;      stage.scaleMode = StageScaleMode.NO_SCALE;      stage.addEventListener(Event.RESIZE, resizeHandler);      stage.addEventListener(MouseEvent.MOUSE_UP, releaseHandler);                  // Tiling Background      var bg:TilingBackground = new TilingBackground();      this.addChild(bg);      bg.init();                          // Preloader      var preloader:Preloader = new Preloader();      this.addChild(preloader);      preloader.init();    }    function load_finished() {      trace("cheese alligator")            // Fridge      var fridge:Fridge = new Fridge();      this.addChild(fridge);      fridge.init();            // Harold and Mabel (and Jalouzie?)      var logo:Logo = new Logo();      this.addChild(logo)      logo.init();      			      var purchase:Purchase = new Purchase();      this.addChild(purchase)      purchase.init();      // Clothesline?    }		function swap_scene() {			_scene = (_scene == null || _scene == "main") ? "buy" : "main";			var x_target = (_scene == "main") ? 0 : -stage.stageWidth;			Tweener.addTween(this, {x:x_target, time:1, transition:"easeInOutCubic"})		}    function resizeHandler(e:Event):void {			// Move fridge			// Resize footer			// etc      trace(stage.stageWidth/2);    }    		function releaseHandler(e:MouseEvent):void {			stopDrag();		}	}	}