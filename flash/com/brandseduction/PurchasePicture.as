package com.brandseduction {  	import flash.display.*;	import flash.filters.*;	import caurina.transitions.Tweener;	import flash.events.*;	import de.popforge.events.SimpleMouseEvent;	import de.popforge.events.SimpleMouseEventHandler;	import flash.net.*		public class PurchasePicture extends MovieClip {	  		function PurchasePicture() {			SimpleMouseEventHandler.register(this);			addEventListener( SimpleMouseEvent.RELEASE, releaseHandler);    }        public function init() {			x = 280;			y = 140;				adapt_to_stage_scaling();			drop_shadow();    }				function drop_shadow() {      var color:Number = 0x000000;      var angle:Number = 45;      var alpha:Number = 1;      var blurX:Number = 10;      var blurY:Number = 10;      var distance:Number = 2;      var strength:Number = 0.85;      var inner:Boolean = false;      var knockout:Boolean = false;      var quality:Number = BitmapFilterQuality.HIGH;      var s:DropShadowFilter = new DropShadowFilter(distance, angle, color, alpha, blurX, blurY, strength, quality, inner, knockout);      filters = [s];		}				public function adapt_to_stage_scaling() {		}				public function shake() {			rotation = -80			Tweener.addTween(this, {rotation:0, time:4, transition:"easeOutElastic"})		}				public function blow_away() {			Tweener.addTween(this, {rotation:-45, time:.75, transition:"easeInCubic"})		}				private function releaseHandler(event:SimpleMouseEvent):void {	      var url:String = "http://rgcreative.com";      var request:URLRequest = new URLRequest(url);			navigateToURL(request, "_blank")		}			}	}