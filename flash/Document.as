package {  	import flash.events.Event;	import flash.display.MovieClip;  import com.brandseduction.*;	public class Document extends MovieClip {				var _container;		var _footer;				public function Document() {			var container:Container = new Container();			container.name = "container"			this.addChild(container);			container.init();			_container = container;			      var footer:Footer = new Footer();			footer.name = "footer"      this.addChild(footer);      footer.init();			_footer = footer;		}	}}